{
  "manifest_version": 3,
  "name": "Restore Twitter Headlines",
  "version": "1.0.01",
  "host_permissions": [
    "https://*/twitter.com/",
    "https://*/x.com/"
  ],

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],

  "icons": {
    "48": "/images/icon_48.png",
    "128": "/images/icon_128.png"
  }
}
